    "prettier-plugin-svelte": "^3.0.3",
    "smui-theme": "7.0.0-beta.15",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "type": "module",
  "dependencies": {
    "@inqling/svelte-icons": "^4.0.2",
    "@tanstack/svelte-query": "^4.35.3",
    "@vite-pwa/sveltekit": "^0.2.7",

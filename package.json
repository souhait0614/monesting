{
  "name": "monesting",
  "version": "0.2.1",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@smui-extra/autocomplete": "^7.0.0-beta.14",
    "@smui-extra/bottom-app-bar": "^7.0.0-beta.14",
    "@smui/button": "7.0.0-beta.14",
    "@smui/card": "^7.0.0-beta.14",
    "@smui/circular-progress": "^7.0.0-beta.14",
    "@smui/common": "^7.0.0-beta.14",
    "@smui/dialog": "^7.0.0-beta.14",
    "@smui/drawer": "^7.0.0-beta.14",
    "@smui/fab": "^7.0.0-beta.14",
    "@smui/icon-button": "^7.0.0-beta.14",
    "@smui/list": "^7.0.0-beta.14",
    "@smui/menu": "^7.0.0-beta.14",
    "@smui/radio": "^7.0.0-beta.14",
    "@smui/snackbar": "^7.0.0-beta.14",
    "@smui/textfield": "^7.0.0-beta.14",
    "@smui/top-app-bar": "^7.0.0-beta.14",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.4",
    "@types/cookie": "^0.5.1",
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-svelte": "^2.32.4",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "smui-theme": "7.0.0-beta.14",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  },
  "type": "module",
  "dependencies": {
    "@inqling/svelte-icons": "^3.4.3",
    "@tanstack/svelte-query": "^4.32.1",
    "@vite-pwa/sveltekit": "^0.2.5",
    "date-fns": "^2.30.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^123.0.0",
    "normalize.css": "^8.0.1",
    "svelte-google-auth": "^0.7.2",
    "svelte-local-storage-store": "^0.5.0",
    "ts-pattern": "^5.0.4",
    "vite-plugin-pwa": "^0.16.4"
  }
}
